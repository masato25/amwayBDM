= render 'shared/layout'
    div class="page-header"
      h2 建立品牌頁面
      = link_to '回上頁', action: "index"
    = render 'shared/notice'
    = form_for @brand do |f|
      div class="form-group" 
        = f.label :BrandName
        = f.text_field :BrandName, class: 'form-control'
      = f.submit '儲存', class: 'btn btn-primary' 
